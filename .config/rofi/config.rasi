/* Generated from ~/dotfiles/system.org */
configuration {
  modi: "filebrowser,window,drun,run,ssh";
  font: "JetBrainsMono\ Nerd\ Font\ 12";
  show-icons: true;
  display-filebrowser: "true";
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method: "name";
  }
}
    
@theme "themes/simple_nord"
